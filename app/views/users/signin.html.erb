<% if @session.errors.any? %>
  <% @user.errors.full_messages.each do |error| %>
    <%= error %><br />
  <% end %>
<% end %>

<%= form_for @session do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br />

  <%= f.label :email%>
  <%= f.text_field :email %><br />

  <%= f.submit "Signin" %>
<% end %>
